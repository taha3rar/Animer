{
  "name": "agt-platform",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build-dev": "ng build --configuration=dev",
    "build-staging": "ng build --configuration=staging",
    "build-prod": "ng build --prod",
    "start": "ng serve -o --configuration=dev",
    "start-local": "ng serve --configuration=local",
    "start-staging": "ng serve --configuration=staging",
    "lint": "ng lint",
    "test": "ng test",
    "test:ci": "npm run lint -s && ng test --code-coverage --browsers=ChromeHeadless",
    "e2e": "ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "postinstall": "npm run prettier -s",
    "precommit": "ng lint && pretty-quick --staged",
    "prepush": "npm run build-staging",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^7.2.16",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.2.16",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@avenews/agt-sdk": "0.0.26",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@ng-bootstrap/ng-bootstrap": "^3.3.0",
    "@ngx-translate/core": "^10.0.1",
    "@types/feather-icons": "^4.7.0",
    "@types/google-libphonenumber": "^7.4.17",
    "@types/jquery": "^3.3.29",
    "@types/socket.io-client": "^1.4.32",
    "angulartics2": "^6.0.0",
    "angularx-social-login": "^1.2.7",
    "bootstrap": "^4.1.1",
    "bootstrap-notify": "3.1.3",
    "bootstrap-select": "^1.13.5",
    "core-js": "^2.5.6",
    "export-to-csv": "^0.2.1",
    "feather-icons": "^4.28.0",
    "file-saver": "^2.0.1",
    "formula-pmt": "^1.0.1",
    "google-libphonenumber": "^3.2.2",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "lodash": "^4.17.10",
    "moment": "^2.23.0",
    "ng-intercom": "^8.0.0",
    "ng-lottie": "^0.3.2",
    "ngrok": "^3.2.5",
    "ngx-bootstrap": "^4.0.0",
    "ngx-pagination": "^3.2.1",
    "ngx-permissions": "^6.0.4",
    "ngx-spinner": "^6.1.2",
    "rxjs": "^6.3.1",
    "sweetalert": "^2.1.2",
    "sweetalert2": "^9.1.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.2",
    "@angular/cli": "~7.0.2",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngx-rocket/scripts": "^3.0.0",
    "@types/file-saver": "^2.0.0",
    "@types/googlemaps": "^3.30.16",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "^2.0.2",
    "@types/lodash": "^4.14.108",
    "@types/node": "^8.10.60",
    "codelyzer": "^4.4.4",
    "hads": "^1.7.0",
    "har-validator": "^5.1.3",
    "htmlhint": "^0.10.0",
    "husky": "^0.14.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "^4.1.0",
    "node-sass": "^4.14.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.11.1",
    "protractor": "~5.4.0",
    "stylelint": "~9.6.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.3.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "~3.1.3"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
